<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="1000.0" styleClass="main" stylesheets="@../../static/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.techstore.controller.ItemController">
   <children>
      <AnchorPane prefWidth="1000.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#store" prefHeight="40.0" prefWidth="200.0" styleClass="button-primary" text="Back" />
            <TableView layoutX="15.0" layoutY="166.0" prefHeight="420.0" prefWidth="970.0">
               <columns>
                  <TableColumn prefWidth="231.0" text="Name" />
                  <TableColumn minWidth="0.0" prefWidth="232.0" text="Billls Issued" />
                  <TableColumn minWidth="0.0" prefWidth="221.0" text="Items Sold" />
                  <TableColumn minWidth="0.0" prefWidth="285.0" text="Profit" />
               </columns>
            </TableView>
            <Label fx:id="cashiers" layoutX="442.0" layoutY="80.0" styleClass="font-secondary" text="Cashiers" />
         </children>
      </AnchorPane>
   </children>
</VBox>
