<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="1000.0" styleClass="main" stylesheets="@../../static/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.techstore.controller.HomeController">
   <children>
      <AnchorPane prefWidth="1000.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="signIn" layoutX="300.0" layoutY="430.0" mnemonicParsing="false" onAction="#signIn" prefHeight="40.0" prefWidth="200.0" styleClass="button-secondary" text="Sign In" />
            <Button fx:id="exitButton" layoutX="300.0" layoutY="530.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" styleClass="button-danger" text="Exit" />
            <Button fx:id="aboutButton" layoutX="300.0" layoutY="480.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" styleClass="button-primary" text="About" />
         </children>
      </AnchorPane>
   </children>
</VBox>
